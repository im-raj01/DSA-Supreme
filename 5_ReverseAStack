#include<bits/stdc++.h>
using namespace std;
#define ll long long

void InsertAtBottom(stack<int>&s,int target){
    if(s.empty()){
        // cout<<"imp";
        s.push(target);
        return;
    }
    int temp = s.top();
    s.pop();
    InsertAtBottom(s,target);

    s.push(temp);
}


void ReverseStack(stack<int>&s){
    if(s.empty()){
        // cout<<"empty";
        return;
    }
    //recursive call
    int temp = s.top();
    s.pop();
    ReverseStack(s);
    //backtracking 
    // s.push(temp);
    int target = temp;
    // s.pop();
    InsertAtBottom(s,target);

}
int main()
{
    stack<int>s;

    s.push(10);
    s.push(20);
    s.push(30);
    s.push(40);
    s.push(50);

    ReverseStack(s);

    while (!s.empty())
    {
        cout<<s.top()<<endl;
        s.pop();
    }
    

 return 0;
}
