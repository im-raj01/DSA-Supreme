#include<bits/stdc++.h>
using namespace std;
#define ll long long

bool isValid(string s){
    if(s.empty()){
        return true;
    }
    stack<char>st;
    for (int i = 0; i < s.size(); i++)
    {
        char ch = s[i];
        if(ch == '(' || ch =='{' || ch == '['){
            st.push(ch);
        }
        else{
            if(!st.empty()){
            char topCh = st.top();
            if(ch==')' && topCh=='('){
                st.pop();
            }
            else if(ch=='}' && topCh=='{'){
                st.pop();
            }
            else if(ch==']' && topCh=='['){
                st.pop();
            }
            else{
                return false;
            }
        }
            else {
                return false;
            }
        }
    }
    if(st.empty()){
        return true;
    }
    else{
        return false;
    }
    
}
int main()
{
    string s;
    cin>>s;

    cout<<isValid(s)<<endl;
 return 0;
}
