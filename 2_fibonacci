#include<bits/stdc++.h>
using namespace std;
#define ll long long

int fib(int n){
    // base case
    if(n== 0 || n== 1){
        return n;
    }
    int ans = fib(n-1) + fib(n-2);
    return ans;
}
int main()
{
 int n;cin>>n;
 int ans = fib(n);
 
 cout<<"Enter the term you want to see\n"<< ans<<endl;
 return 0;
}
